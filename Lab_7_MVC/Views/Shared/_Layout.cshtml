<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.js"></script>
</head>
<body>
<!--меню-->
<nav class="navbar navbar-inverse">
    <div class="container">
        <div>
            <ul class="nav navbar-nav">
                <li><a href="@Url.Action("Index","Home")" class="navbar-brand">Главная</a></li>
                <li><a href="@Url.Action("Contact", "Home")" class="navbar-brand">Контакты</a></li>
                <li><a href="@Url.Action("About","Home")" class="navbar-brand">Время</a></li>
                @RenderSection("Menu")
            </ul>
        </div>
    </div>
</nav>
<!--основной контент-->
<div class="container body-content">
    @RenderBody()
    <hr/>
    <footer>
        <p>tavi</p>
        @RenderSection("Footer", false) 
    </footer>
</div>
</body>
</html>